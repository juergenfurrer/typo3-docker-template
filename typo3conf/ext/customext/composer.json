{
    "name": "swisscode/customext",
    "type": "typo3-cms-extension",
    "description": "Custom extension",
    "authors": [
        {
            "name": "SwissCode",
            "role": "Developer"
        }
    ],
    "require": {
        "typo3/cms-core": "^8.7.1"
    },
    "autoload": {
        "psr-4": {
            "Swisscode\\Customext\\": "Classes"
        }
    },
    "replace": {
        "bcext": "self.version",
        "typo3-ter/bcext": "self.version"
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "customext"
        }
    }
}
